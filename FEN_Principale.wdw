   -
     name : AjouteAbonnéSupplémentaire
     internal_properties : 
     procedure_id : 1964536427990321632
     type_code : 14
     group : 5
     code : |1-
      // Résumé : Ajoute un abonné supplémentaire (réplica autre que Espagne ou Portugal)
      // Syntaxe :
      // AjouteAbonnéSupplémentaire ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AjouteAbonnéSupplémentaire()
      
      sPays est une chaine
      sRéplicaMaitre est une chaine
      sRepertoireNouvelAbonne est une chaine
      sRéplicaTransportable est une chaine
      nNouvelAbonne est un entier
      
      
      // Sélection du pays
      sPays = SansEspace(Majuscule(Ouvre(FEN_SélectionPays)))
      // Si un pays est sélectionné
      SI sPays <> "" ALORS
      	
      	// Création du répertoire d'accueil des données
      	sRepertoireNouvelAbonne = pGlobales.gsRepertoireBase +["\"]+ sPays + "\"
      	
      	// Crée le répertoire abonné
      	fRepCrée(sRepertoireNouvelAbonne)
      	
      	// Création du réplica abonné pour le site
      	ErreurChangeParamètre(epCode)
      	SI hCréeRéplicaAbonné(pGlobales.gsRepertoireMaitre, sRepertoireNouvelAbonne, sPays, 0, "CLIENT" +TAB+ "IDDATEHEURE") = faux ALORS
      		// Il n'a pas été possible de créer l'abonné
      		// Traitement du cas ou il a déjà été créé dans ce cas il faut le recréer, sinon abandon de la création avec affichage de la cause...
      		SI ErreurInfo(errCode) = 180020 ALORS
      			// Recréation d'un abonné déjà créé précédemment ...
      			SI HRecréeRéplicaAbonné(pGlobales.gsRepertoireMaitre, sRepertoireNouvelAbonne, sPays) = faux ALORS
      				FinProgramme("Echec de création du réplica abonné." +RC+ ErreurInfo())
      			FIN
      		SINON 
      			FinProgramme("Echec de création du réplica abonné." +RC+ ErreurInfo())
      		FIN
      	FIN
      	ErreurChangeParamètre(epAfficheMessageFinProgramme)
      	
      	
      	// Transfert de la base HyperFileSQL (maître) dans une nouvelle base correspond à ce nouvel abonné (première synchronisation)
      	sRéplicaMaitre = pGlobales.gsRepertoireMaitre +["\"]+ cstNomReplicaMaitre
      	
      	// Ouverture d'une connexion sur la base HyperFileSQL (Portugal - Maître)
      	OuvreUneConnexion("PORTUGAL")
      	
      	// Création du réplica transportable avec contenu initial de la base HyperFileSQL
      	HCréeRéplicaTransportable(sRéplicaMaitre, sPays, "")
      	
      	// Ouverture d'une connexion HyperFileSQL dans le dossier des données du nouvel abonné
      	OuvreUneConnexion(sPays)
      	sRéplicaTransportable = pGlobales.gsRepertoireMaitre +["\"]+ RPL.Fichier
      	
      	// Création du fichier de données  : la base Access
      	HCréation(CLIENT)
      	
      	// Synchronisation du réplica avec la base Access
      	HSynchroniseRéplica(sRéplicaTransportable, sRepertoireNouvelAbonne + "Replica_"+sPays+".RPL", rplVersAbonné)
      	
      	// Il faut effectuer une synchronisation de l'abonné vers le maître également
      	// Le maître sera ainsi "informé" que la synchronisation initiale a bien été faite
      	HCréeRéplicaTransportable(sRepertoireNouvelAbonne + "Replica_"+sPays+".RPL", sPays, "")
      	sRéplicaTransportable = sRepertoireNouvelAbonne +["\"]+ RPL.Fichier
      	
      	// Connexion à la base maître afin de synchroniser ce nouveau réplica abonné...
      	OuvreUneConnexion("PORTUGAL")		
      	HSynchroniseRéplica(pGlobales.gsRepertoireMaitre +["\"]+ cstNomReplicaMaitre, sRéplicaTransportable, rplVersMaître)
      	
      	// Mise à jour de la liste des réplicas créés afin de pouvoir les manipuler...
      	AfficheAbonnésSupplémentaires()
      	
      	nNouvelAbonne = ZoneRépétéeCherche("ATT_LIB_PAYS", sPays, Vrai, 1)
      	ZR_ABONNE = nNouvelAbonne
      	ZoneRépétéePosition(ZR_ABONNE, nNouvelAbonne)
      	
      	Info(ChaîneConstruit("Un nouveau jeu de données a été créé pour le pays : %1." +RC+ "Il est localisé dans le dossier : %2." +RC+RC+ "Vous pouvez consulter et modifier les données de ce pays, puis les synchroniser avec la base maître.", sPays, sRepertoireNouvelAbonne))
      	
      FIN
     type : 458752
   -
     name : SupprimeAbonnéSupplémentaire
     procedure_id : 1964536805947531337
     type_code : 14
     group : 5
     code : |1+
      // Résumé :  Suppression du dossier correspondant à un abonné (et toutes ses données ...)
      // Syntaxe :
      // SupprimeAbonnéSupplémentaire ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SupprimeAbonnéSupplémentaire()
      
      sPays est une chaine
      sRepertoireAbonné est une chaîne
      
      
      // Si un abonné est sélectionné
      SI ZR_ABONNE > 0 ALORS
      	
      	// Mémorise le pays de l'abonné
      	sPays = ATT_LIB_PAYS[ZR_ABONNE]
      	
      	// Mémorise le répertoire associé
      	sRepertoireAbonné = pGlobales.gsRepertoireBase +["\"]+ sPays +"\"
      	
      	//1 : &Supprimer
      	//2 : &Ne pas supprimer
      	SELON Dialogue(<§?1B43701D02D709C9§>, sPays, sRepertoireAbonné)
      		// &Supprimer
      		CAS 1
      			// Ferme le fichier
      			HFerme(CLIENT)
      			// Supprime le répertoire et ses fichiers
      			SI fRepSupprime(sRepertoireAbonné, frRécursif+frLectureSeule) ALORS
      				// Supprime l'abonné de la zone répétée
      				ZoneRépétéeSupprime(ZR_ABONNE, ZR_ABONNE)
      				// Message d'information
      				Info(ChaîneConstruit("%1 : toutes les données ont été supprimées.", sPays))
      			SINON
      				// Message d'erreur
      				Erreur("La suppression n'a pas été faite.", erreurinfo())	
      			FIN
      			
      		// &Ne pas supprimer
      		CAS 2
      			// Ne rien faire
      	FIN
      
      FIN
