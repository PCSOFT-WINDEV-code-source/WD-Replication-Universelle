  procedures :
   -
     name : AfficheClient
     procedure_id : 1964520498386956538
     type_code : 14
     code : |1-
      // Résumé : Ouvre la fenêtre de visualisation des clients d'un pays
      // Syntaxe :
      //AfficheClient (<sPays> est chaîne)
      //
      // Paramètres :
      //	sPays (chaîne ANSI) : Pays pour lequel les données doivent être visualisés
      // Valeur de retour :
      // 	Aucune
      //
      Procédure AfficheClient(LOCAL sPays est une chaine)
      
      // Ouvre la fenêtre de visualisation
      ouvre(FEN_Table_CLIENT, sPays)
     type : 458752
   -
     name : AfficheAbonnésSupplémentaires
     procedure_id : 1964520498387022074
     type_code : 14
     group : 5
     code : |1+
      // Résumé : Liste les différents abonnés créés précédemment, autres que celui par défaut (Espagne).
      // Syntaxe :
      // AfficheAbonnésSupplémentaires ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure AfficheAbonnésSupplémentaires()
      
      sListeAbonnésSupplémentaires est une chaine
      sRepertoireAbonneSupplémentaire est une chaine
      
      
      // Vide la zone répétée des abonnés
      ZoneRépétéeSupprimeTout(ZR_ABONNE)
      
      // Récupère la liste des abonnés supplémentaires
      sListeAbonnésSupplémentaires = ListeAbonnesSupplementaires()
      
      // Parcours la liste des répertoires
      POUR TOUTE CHAINE sUnAbonneSupplementaire DE sListeAbonnésSupplémentaires SEPAREE PAR RC
      	
      	si sUnAbonneSupplementaire dans ("", eot) alors continuer
      	
      	// Supprime de l'arborescence le répertoire de base
