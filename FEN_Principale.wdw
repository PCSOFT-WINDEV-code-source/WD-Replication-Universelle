   -
     code : |1+
      procedure FEN_Principale()
      
   -
     code : |1-
      // Initialise les informations (répertoires, connexions)
      InitialiseRepertoiresEtConnexions()
      
      // Initialisation curseurs sur images des bases pour faciliter la compréhension
      IMG_BaseMaitre_1..CurseurSouris = curMainSystème
      IMG_BaseAbonne_1..CurseurSouris = curMainsysteme
      
      // Si l'initialisation n'a pas été faite, on grise les options qui ne peuvent pas s'exécuter...
      SI fRep(pGlobales.gsRepertoireTransfert, frRépertoire) <> "" ALORS
      	GRP_INIT_OK..Etat = Actif
      SINON
      	GRP_INIT_OK..Etat = Grisé
      FIN
      
      // Listes les différents abonnés créés précédemment, autres que celui par défaut (Espagne).
      AfficheAbonnésSupplémentaires()
      
      // Affichage la fenêtre complète s'il y a déjà eu la création de différents abonnés, ou une fenêtre réduite ...
      TIR_Abonnes = (ZoneRépétéeOccurrence(ZR_ABONNE) <> 0)
