     code : |1-
      // Résumé : Transmet les données du maître vers un abonné
      // Syntaxe :
      //TransmettreDonnéesMaitre ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE TransmettreDonnéesMaitre()
      
      sRéplicaMaitre est une chaine
      sAbonné est une chaine
      
      
      // Définit le nom du fichier réplica maître
      sRéplicaMaitre = pGlobales.gsRepertoireMaitre +["\"]+ cstNomReplicaMaitre
      
      // Ouverture d'une connexion sur la base HyperFileSQL Portugal
      // Ainsi le programme s'exécute comme si l'on se trouvait sur le site au  Portugal
      // avec la base de données HyperFileSQL
      SI pGlobales.gsConnexionCourant <> pGlobales.gsNomConnexion_Portugal ALORS OuvreUneConnexion("PORTUGAL")
      
      // Sélection de l'abonné pour lequel on souhaite créer un réplica transportable afin de lui envoyer les données modifiées/créées dans la base maître ...
      sAbonné = ListeAbonnesSupplementaires()
      // S'il n'existe aucun abonné supplémentaire
      SI sAbonné = "" ALORS
      	// Définit l'abonné destinataire comme celui par défaut
      	sAbonné = "ESPAGNE"
      SINON
      	// Demande à l'utilisateur de sélectionner l'abonné destinataire
      	sAbonné = Ouvre(FEN_SélectionPays, "ESPAGNE" +RC+ sAbonné)
      	// Si aucun abonné n'a été sélectionné
      	SI sAbonné = "" ALORS
      		Info("Aucun abonné n'a été sélectionné, aucun réplica transportable n'est donc créé.")
      		RETOUR
      	FIN
      FIN
      
      // Création du réplica transportable pour l'abonné destinataire
      SI HCréeRéplicaTransportable(sRéplicaMaitre, sAbonné, "") ALORS
      	
      	// Copie le réplica transportable dans le répertoire de transfert
      	SI fCopieFichier(pGlobales.gsRepertoireMaitre +["\"]+ RPL.Fichier, pGlobales.gsRepertoireTransfert +["\"]+ RPL.Fichier ) ALORS
      		// Message d'information
      		Info(ChaîneConstruit("Le réplica transportable %1 a été placé dans le répertoire suivant : %2", RPL.Fichier, pGlobales.gsRepertoireTransfert), "Le site abonné peut maintenant récupérer ce réplica transportable pour récupérer les données modifiées.")
      	SINON
      		// Message d'erreur
      		Erreur("Echec de copie du réplica transportable dans le répertoire de transfert", ErreurInfo(errmessage))
      	FIN
      	
      SINON
      	
      	// Message d'erreur
      	Erreur("Echec de création d'un réplica transportable.", hErreurInfo(herrmessage))
      	
      FIN
